<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:v="clr-namespace:SatisfactorySmartHub.Presentation.Views"
    xmlns:vm="clr-namespace:SatisfactorySmartHub.Application.PresentationModels.ViewModels;assembly=SatisfactorySmartHub.Application">

    <DataTemplate DataType="{x:Type TypeName=vm:HubViewModel}">
        <v:HubView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type TypeName=vm:AdminViewModel}">
        <v:AdminView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type TypeName=vm:CorporationViewModel}">
        <v:CorporationView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type TypeName=vm:BranchViewModel}">
        <v:BranchView />
    </DataTemplate>
</ResourceDictionary>